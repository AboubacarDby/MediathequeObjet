package mediatheque;

public class Acteur {
    private String prenom;
    private String nom;

    public Acteur(String prenom, String nom) {
	super();
	this.prenom = prenom;
	this.nom = nom;
    }

    public Acteur() {
	super();
    }

    public String getPrenom() {
	return prenom;
    }

    public void setPrenom(String prenom) {
	this.prenom = prenom;
    }

    public String getNom() {
	return nom;
    }

    public void setNom(String nom) {
	this.nom = nom;
    }

}
